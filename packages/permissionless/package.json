        "./errors": {
            "types": "./_types/errors/index.d.ts",
            "import": "./_esm/errors/index.js",
            "default": "./_cjs/errors/index.js"
        },
        "./experimental/pimlico": {
            "types": "./_types/experimental/pimlico/index.d.ts",
