    "types": "./_types/index.d.ts",
    "typings": "./_types/index.d.ts",
    "type": "module",
    "sideEffects": false,
    "description": "A utility library for working with ERC-4337",
    "keywords": [
        "ethereum",
        "erc-4337",
        "eip-4337",
        "paymaster",
